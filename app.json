{
  "expo": {
    "name": "cascadia-tracker",
    "slug": "cascadia-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/nature-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/background-mobile.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "br.alessi.cascadiatracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/nature-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icons/nature-icon.png"
    }
  }
}
